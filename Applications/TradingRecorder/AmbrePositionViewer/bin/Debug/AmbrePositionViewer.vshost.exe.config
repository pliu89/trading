<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.diagnostics>
    <switches>
      <add name="TT API" value="Verbose" />
      <add name="TT API Orders" value="True" />
      <add name="TT API Fills" value="True" />
      <add name="TT API General" value="True" />
    </switches>
    <sources>
      <source name="TT API">
        <listeners>
          <add name="consoleListener"/>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="consoleListener" type="System.Diagnostics.ConsoleTraceListener" traceOutputOptions="DateTime, ProcessId, ThreadId"/>
    </sharedListeners>
    <trace autoflush="true"/>
  </system.diagnostics>
</configuration>